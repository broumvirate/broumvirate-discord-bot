while (tweetindex.exists({
            sort: {$gte: tweetindex.countDocuments()}
        })) {
        tweetindex.find({}, async function (err, tweets) {
            if (err) {
                console.log(err)
            } else {
                for (var i = 0; i < tweets.length; i++) {
                    if (tweetindex.exists({
                        sort: tweets[i].sort - 1
                    })){
                        tweets[i].sort--;
                        tweets[i].save();
                        console.log(tweets[i]);
                    }
                }
            }

        })
    }